/* pages/home/proDetail/proDetail.wxss */

.proDetailContainer {
  display: flex;
  flex-direction: column;
}

.tabBar {
  width: 100%;
  height: 100rpx;
  display: flex;
  flex-direction: row;
  position: fixed;
  bottom: 0;
  border-top:1px solid #ececec;
}

.tabBar .right {
  color: #fff;
  font-size: 34rpx;
  background-color: #3bc53a;
  height: 100%;
  line-height: 100rpx;
  width: 60%;
  text-align: center;
}

.tabBar .left {
  background-color: #fff;
  width: 40%;
  line-height: 100rpx;
  text-align: center;
  height: 100%;
  font-size: 34rpx;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}

.tabBar .left image {
  height: 40rpx;
  width: 36rpx;
  margin-top: 5rpx;
  margin-right: 20rpx;
}
