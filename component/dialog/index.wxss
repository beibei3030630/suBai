/* component/dialog/index.wxss */
.dialogContainer{
  background-color: #fff;
  position: fixed;
  margin:  auto;
  top: 0;
  right: 0;
  left: 0;
  bottom: 20%;
  width: 75%;
  height: 300rpx;
  border-radius: 50rpx;
  display: flex;
  justify-content: space-around;
  flex-direction: column;
  align-items: center;
  z-index: 100;
}
.dialogContainer .title{
  margin-top: 40rpx;
  font-size: 34rpx;
}
.dialogContainer .content{
  margin: 15rpx 0;
  font-size: 28rpx;
  color: #9b9b9b;
}
.dialogContainer .btnList{
  width: 100%;
  display: flex;
  flex-direction: row;
  border-top: 1rpx solid #ececec;
  justify-content: space-around;
  align-items: center;
  font-size: 36rpx;
}
.dialogContainer .btnList>view{
  padding: 20rpx;
  width: 50%;
  text-align: center;
}
.dialogContainer .cancelBtn{
  letter-spacing: 2rpx;
}
.dialogContainer .confirmBtn{
  letter-spacing: 2rpx;
  color: #09BB07;
  border-left: 1rpx solid #ececec;
}
