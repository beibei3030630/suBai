/* component/ticketList/index.wxss */

 .ticketItem {
  border-radius: 20rpx;
  width: 90%;
  background-color: #fff;
  display: flex;
  flex-direction: row;
  padding: 30rpx 30rpx;
  justify-content: space-between;
  align-items: center;
  box-sizing: border-box;
  margin-bottom: 30rpx;
}

.ticketItem .left>view:nth-child(1) {
  font-size: 36rpx;
  text-align: center;
  margin-bottom: 10rpx;
}

.ticketItem .left>view:nth-child(2) {
  font-size: 24rpx;
  color: #9b9b9b;
}

.ticketItem .center>view:nth-child(1) {
  font-size: 36rpx;
  text-align: left;
  margin-bottom: 10rpx;
}

.ticketItem .center>view:nth-child(2) {
  font-size: 24rpx;
  color: #666;
}

.ticketItem .right image {
  width: 55rpx;
  height: 55rpx;
}